<!DOCTYPE html>
<html>
    <head>
        <title>Navigator</title>
        <script>
          document.write("appname "+ navigator.appName+"<br>");
          document.write("appcode "+ navigator.appCodeName+"<br>");
          document.write("cookies "+ navigator.cookieEnabled+"<br>");

          for(n in navigator)
          document.write(n+"<br>");
        </script>
    </head>
    <body>
       
        
    </body>
</html>
